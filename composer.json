{
    "name": "pos-lifestyle/laravel-nova-date-range-filter",
    "description": "A Laravel Nova filter.",
    "keywords": [
        "laravel",
        "nova",
        "date",
        "date range",
        "filter"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "laravel/framework": "^5.8|6.*",
        "laravel/nova": "2.*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "PosLifestyle\\DateRangeFilter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PosLifestyle\\DateRangeFilter\\FilterServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
